{
    "name": "tinghom/laravel-avoid-resubmit",
    "description": "A package that is applied to Laravel to avoid resubmitting form data",
    "keywords": [
        "laravel",
        "resubmit"
    ],
    "authors": [
        {
            "name": "tinghom",
            "email": "tinghom@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Tinghom\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tinghom\\Middleware\\AvoidResubmitServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
